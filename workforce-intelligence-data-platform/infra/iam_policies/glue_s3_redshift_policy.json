{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:*"
      ],
      "Resource": [
        "arn:aws:s3:::your-etl-bucket",
        "arn:aws:s3:::your-etl-bucket/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "redshift:*"
      ],
      "Resource": "*"
    }
  ]
}